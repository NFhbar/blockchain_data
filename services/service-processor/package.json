{
  "name": "@blockchain/service-processor",
  "version": "1.0.0",
  "description": "Service Processor",
  "main": "index.js",
  "scripts": {
    "getName": "echo $npm_package_name",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "db:clean": "npm run db:seed:undo && npm run db:migrate:undo",
    "local:start": "./bin/run-app.sh",
    "development:seed:migrate": "npm run db:clean && npm run db:migrate && npm run db:seed && npm run development",
    "development": "cross-env NODE_PATH=. NODE_ENV=development nodemon --trace-warnings -r esm index.js",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "echo nothing yet",
    "precommit": "lint-staged",
    "docs": "./node_modules/.bin/jsdoc -r src -d ./docs --readme ./README.md"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awilix": "^3.0.9",
    "awilix-express": "^0.11.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "device-ip-location": "^1.0.27",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.22.0",
    "http-status": "^1.4.2",
    "https": "^1.0.0",
    "mathjs": "^9.2.0",
    "method-override": "^3.0.0",
    "paginate-info": "^1.0.4",
    "pathfinder-ui": "^1.0.3",
    "pg": "^8.2.1",
    "sequelize": "^5.21.12",
    "sequelize-cli": "^5.5.1",
    "structure": "^2.0.0",
    "uuid": "^8.3.1",
    "validator": "^13.1.17",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "express-status-monitor": "^1.3.3",
    "lint-staged": "^8.2.0",
    "jsdoc": "^3.6.5",
    "nodemon": "^2.0.4",
    "standard": "^14.3.4"
  }
}
